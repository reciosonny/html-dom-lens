{
  "name": "webpackplayground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "webpack": "webpack --config=config/webpack.config.js",
    "build": "npm run clean && webpack --config=config/webpack.config.js --mode development --env.mode development",
    "prod": "npm run clean && webpack --mode production --config=config/webpack.config.js --env.mode production --display-used-exports",
    "prod:analyze": "npm run clean && webpack --mode production --config=config/webpack.config.js --env.mode production --presets analyze --display-used-exports",
    "web:dev": "webpack-dev-server --hotOnly --config=config/webpack.config.js --mode development --env.mode development --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://reciosonny@bitbucket.org/reciosonny/webpack-playground.git"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/reciosonny/webpack-playground#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.0.2",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "file-loader": "^6.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "react-hot-loader": "^4.12.20",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.22.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.1.3",
    "http-proxy-middleware": "^0.19.0",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "reactstrap": "^6.4.0"
  }
}
